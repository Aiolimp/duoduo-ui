{
    "name": "duoduo-ui",
    "version": "1.0.0",
    "description": "多多组件库",
    "main": "index.js",
    "scripts": {
        "lint:es": "eslint packages/**/*.js",
        "spellcheck": "cspell lint --dot --gitignore --color --cache --show-suggestions \"(packages|apps)/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md)\"",
        "lint:style": "stylelint \"{packages,apps}/**/*.{css,ts}\"",
        "prepare": "husky",
        "commit": "git-cz",
        "lint": "npm run lint:es && npm run lint:style"
    },
    "keywords": [],
    "author": "duiduo",
    "license": "ISC",
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "devDependencies": {
        "eslint": "9.5.0",
        "@eslint/js": "9.5.0",
        "typescript-eslint": "7.14.1",
        "cspell": "8.9.1",
        "stylelint": "16.6.1",
        "husky": "9.0.11",
        "commitlint": "19.3.0",
        "cz-git": "1.9.3",
        "commitizen": "4.2.4",
        "@commitlint/config-conventional": "19.2.2"
    }
}
